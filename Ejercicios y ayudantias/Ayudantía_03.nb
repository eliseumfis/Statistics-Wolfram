Cell[BoxData[
 RowBox[{"\n", 
  RowBox[{
   RowBox[{
    RowBox[{"mean", "=", 
     RowBox[{"n", "*", "p"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"stddev", "=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"n", "*", "p", "*", 
       RowBox[{"(", 
        RowBox[{"1", "-", "p"}], ")"}]}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"normalDist", "=", 
     RowBox[{"NormalDistribution", "[", 
      RowBox[{"mean", ",", "stddev"}], "]"}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{"aproximaci\[OAcute]n", " ", "normal"}], "*)"}], "\n", "\n", "\n", 
   RowBox[{
    RowBox[{"normalData", "=", 
     RowBox[{"RandomVariate", "[", 
      RowBox[{"normalDist", ",", "1000"}], "]"}]}], ";", " ", 
    RowBox[{"(*", 
     RowBox[{
     "muestra", " ", "de", " ", "distribuci\[OAcute]n", " ", "normal"}], 
     "*)"}], "\n", "\n"}]}]}]], "Input",
