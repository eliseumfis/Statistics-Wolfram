Cell[TextData[{
 "\nEl histograma muestra la distribuci\[OAcute]n binomial y la aproximaci\
\[OAcute]n normal. Como podemos ver, a medida que ",
 StyleBox["n",
  FontSlant->"Italic"],
 " se hace m\[AAcute]s grande, ",
 "la distribuci\[OAcute]n binomial se parece cada vez m\[AAcute]s a la \
distribuci\[OAcute]n normal. En este caso, con \[OpenCurlyDoubleQuote]n = 100\
\[CloseCurlyDoubleQuote], ya podemos apreciar una buena aproximaci\[OAcute]n. \
Ve\[AAcute]moslo con otros valores...\n"
}], "Text",
 CellChangeTimes->{
  3.9036514346778927`*^9, {3.903651465809217*^9, 3.903651520674283*^9}, {
   3.903651578316074*^9, 
   3.903651592262817*^9}},ExpressionUUID->"429b46d4-efd0-4068-bdf1-\
bf3064d02949"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"n", "=", "100"}], ";", 
  RowBox[{"p", "=", "0.5"}], ";", 
  RowBox[{"data", "=", 
   RowBox[{"RandomVariate", "[", 
    RowBox[{
     RowBox[{"BinomialDistribution", "[", 
      RowBox[{"n", ",", "p"}], "]"}], ",", "1000"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mean", "=", 
   RowBox[{"n", "*", "p"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"stddev", "=", 
   RowBox[{"Sqrt", "[", 
    RowBox[{"n", "*", "p", "*", 
     RowBox[{"(", 
      RowBox[{"1", "-", "p"}], ")"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"normalDist", "=", 
   RowBox[{"NormalDistribution", "[", 
    RowBox[{"mean", ",", "stddev"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"normalData", "=", 
   RowBox[{"RandomVariate", "[", 
    RowBox[{"normalDist", ",", "1000"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Histogram", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"data", ",", "normalData"}], "}"}], ",", "Automatic", ",", 
   "\"\<PDF\>\"", ",", 
   RowBox[{
   "PlotLabel", "->", 
    "\"\<Distribuci\[OAcute]n Binomial vs. Aproximaci\[OAcute]n Normal\>\""}],
    ",", 
   RowBox[{"ChartLegends", "->", 
    RowBox[{"{", 
     RowBox[{"\"\<Binomial\>\"", ",", "\"\<Normal\>\""}], "}"}]}]}], 
  "]"}], "\n"}], "Input",
